@model Cliente
@{
    ViewData["Title"] = "Adopcion animales";
}
<h1>@ViewData["Title"]</h1>

<p>Adopte uno de nuestros animales! c:</p>

<form method="post">

    <div class="mb-3">
        <label>Cedula</label>
        <input asp-for="Cedula" class="form-control" disabled>
    </div>
    <div class="mb-3">
        <label>Nombre</label>
        <input asp-for="Nombre" class="form-control" disabled>
    </div>
    <div class="mb-3">
        <label>Animal comprado</label>
        <input asp-for="AnimalComprado" class="form-control" >
    </div>


    <div class="row col-6">
        <div class="col-6 m-auto">
            <button type="submit" class="btn btn-primary">Guardar</button>
        </div>
        <div class="col-6 m-auto">
            <a asp-action="Index" class="btn btn-outline-primary">Regresar</a>
        </div>
    </div>

</form>

